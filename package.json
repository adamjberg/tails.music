{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run generate-blog && bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run generate-blog && bun run build.ts",
    "generate-blog": "bun run scripts/generate-blog-data.ts"
  },
  "dependencies": {
    "bun-plugin-tailwind": "^0.0.15",
    "gray-matter": "^4.0.3",
    "marked": "^16.3.0",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^7.8.2",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/marked": "^6.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
